<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.text.Text?>
<?import java.net.URL?>
<VBox xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.carmen.algorithmtesting.WelcomeController"
            prefHeight="400.0" prefWidth="600.0" alignment="CENTER">




    <VBox fx:id="welcomeBox" alignment="CENTER" styleClass="welcomeBox" >
        <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </padding>

        <Text fx:id="welcomeText" text="Welcome to the Algorithm Testing Application!"/>
        <Label fx:id="welcomeMessage" text="welcome message"/>
        <Button fx:id="startButton" text="Start testing" styleClass="welcomeButton" onAction="#goToMainScreen"/>

    </VBox>

    <stylesheets>
        <URL value="@../css/styles.css"/>
    </stylesheets>


</VBox>
